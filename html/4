<script type="application/javascript">
	(function(){
		var loader = document.getElementById('welcome-loading');
		if ( loader ) {
			loader.style.width = '70%';
		}
				var loader2 = document.getElementById('alternate-loader-line');
		if ( loader2 ) {
			var loaderStrokeLength	= loader2.getTotalLength();
			var progress = 0;
			var interval = setInterval(function(){
				progress += .2;
				loader2.style.strokeDashoffset = Math.floor(loaderStrokeLength * (1 - progress));
				loader2.style.strokeDasharray = Math.ceil(loaderStrokeLength * progress * 1000 ) / 1000 + ', ' + Math.floor(loaderStrokeLength * (1 - progress) * 1000 ) / 1000;
				if ( progress === 1) {
					clearInterval(interval);
					if ( window.app ) {
						app.on('pageAnimated', function() {
							loader2.style.strokeDashoffset = 0;
							loader2.style.strokeDasharray = '0, ' + loaderStrokeLength;
						});
					}
				}
			}, 40);
		}
			})();
</script>

