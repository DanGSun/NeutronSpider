//console.info('filterProjects: ', ObjectCompiler(projects));
localStorage.setItem('filterProjects', JSON.stringify(ObjectCompiler(projects)));

//console.info('filterSpecializations: ', ObjectCompiler(specializations));
localStorage.setItem('filterSpecializations', JSON.stringify(ObjectCompiler(specializations)));

//console.info('currentSpecializations: ', ObjectFromOjectsCompiler(currentSpecializations));
