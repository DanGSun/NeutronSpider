careerJSON = Object.assign(ObjectCompiler(cities), ObjectCompiler(projects), ObjectCompiler(specializations));

function ObjectCompiler(dataArray) {
	var compiledJSON = {};
	for( var i = 0, max = dataArray.length; i < max; i++ ) {
		compiledJSON[dataArray[i]['id']] = dataArray[i]['name'];
	}
	return compiledJSON;
}

localStorage.setItem('careerJSON', JSON.stringify(careerJSON));

function ObjectFromOjectsCompiler(dataArray) {
	var compiledJSON = {};
	for( var i = 0, max = dataArray.length; i < max; i++ ) {
		compiledJSON[i] = dataArray[i];
	}
	return compiledJSON;
}

</script>
